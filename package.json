{
    "name": "simple-animate",
    "version": "0.0.3",
    "description": "Simple and minimal animation library",
    "scripts": {
        "lint": "eslint src/",
        "prepublish": "npm run lint && node rollup.build.js",
        "test": "ava --tap | tap-nyan"
    },
    "main": "index.js",
    "module": "./src/animate.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/christiansany/simple-animate.git"
    },
    "keywords": [
        "animate",
        "animation",
        "simple"
    ],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/christiansany/simple-animate/issues"
    },
    "homepage": "https://github.com/christiansany/simple-animate#readme",
    "devDependencies": {
        "ava": "^0.21.0",
        "babel-cli": "^6.24.1",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-es2015": "^6.24.1",
        "babelrc-rollup": "^3.0.0",
        "eslint": "^4.3.0",
        "eslint-config-airbnb-base": "^11.3.1",
        "eslint-plugin-import": "^2.7.0",
        "rollup": "^0.45.2",
        "rollup-plugin-babel": "^2.7.1",
        "rollup-plugin-uglify": "^2.0.1",
        "tap-nyan": "^1.1.0"
    },
    "ava": {
        "files": [
            "src/**/*.spec.js"
        ],
        "source": [
            "src/**/*.js"
        ],
        "concurrency": 5,
        "failFast": true,
        "powerAssert": true,
        "require": [
            "babel-register"
        ],
        "babel": "inherit"
    }
}
